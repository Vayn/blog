---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>NextGen</title>
    <link href="http://elnode.com/atom.xml" rel="self"/>
    <link href="http://elnode.com/"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://elnode.com/</id>
    <author>
        <name>Ben Ubois</name>
        <email>ben@elnode.com</email>
    </author>

    {% for post in site.posts %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://elnode.com{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://elnode.com{{ post.id }}</id>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}

</feed>